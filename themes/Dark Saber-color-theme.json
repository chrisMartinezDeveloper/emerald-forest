      "settings": {
        "foreground": "#e59071f3"
      }
    },
    {
      "name": "Function: Parameters",
      "scope": ["variable.parameter"],
      "settings": {
        "foreground": "#e59071f3"
      }
    },
    {
      "name": "Number, Constant, Function Argument, Tag Attribute, Embedded",
